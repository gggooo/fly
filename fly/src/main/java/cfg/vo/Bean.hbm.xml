<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cfg.vo.Bean" table="cfg_bean">
		<id name="id"><column name="id" /><generator class="uuid.hex" /></id>
		<property name="bean" column="bean"/>
		<property name="descrProperty" column="descrProperty"/>
		<property name="descr" column="descr"/>
		<property name="tableName" column="tableName"/>
		<property name="isFlow" column="isFlow"/>
		<property name="pkg" column="pkg"/>
		<property name="usrct" column="usrct"/>
	</class>
</hibernate-mapping>